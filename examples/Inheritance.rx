# vim: set ft=rb:

class @
  def @main()
    def point2d = new Point2d(100, 200)
    trace(point2d.toString())

    def point3d = new Point3d(100, 200, 150)
    trace(point3d.toString())
  end
end

class Point2d
  def x: Int
  def y: Int

  def new(x, y)
    @x = x
    @y = y
  end

  def toString()
    "Point2d: x=#x, y=#y"
  end
end

class Point3d < Point2d
  def z: Int

  def new(x, y, z)
    super(x, y)
    @z = z
  end

  override def toString()
    "Point3d: x=#x, y=#y, z=#z"
  end
end
