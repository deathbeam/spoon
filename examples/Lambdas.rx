self class

def self.main()
  def numbers = [1, 3, 5, 6, 7, 8]

  trace(numbers.count()) -- 6
  trace(numbers.has(4)) -- false

  -- test if all numbers are greater/smaller than 20
  trace(numbers.foreach(do(v)
    return v < 20
  end)) -- true

  trace(numbers.foreach(do(v)
    return v > 20
  end)) -- false

  -- sum all the numbers
  def sum = do(num, total) 
    return total += num
  end

  trace(numbers.fold(sum, 0)) -- 30
end