#!/usr/bin/env coffee

class Point2d
  new = (x, y) ->
    @x as Int = x
    @y as Int = y

  to-string = ->
    "Point2d: x=#{@x}, y=#{@y}"

point2d = Point2d(100, 200)
trace point2d.to-string!

class Point3d < Point2d
  new = (x, y, z) ->
    super x, y
    @z as Int = z

  :override
  to-string = ->
    "Point3d: x=#{@x}, y=#{@y}, z=#{@z}"

point3d = Point3d(100, 200, 150)
trace point3d.to-string!
