#!/usr/bin/env coffee



class Point2d
  x as Int
  y as Int

  new = (x, y) ->
    @x = x
    @y = y

  to-string = ->
    "Point2d: x=#{@x}, y=#{@y}"

point2d = Point2d(100, 200)
trace point2d.to-string!

point3d = Point3d(100, 200, 150)
trace point3d.to-string!

class Point3d < Point2d
  z as Int

  new = (x, y, z) ->
    super(x, y)
    @z = z

  :override
  to-string = ->
    "Point3d: x=#{@x}, y=#{@y}, z=#{@z}"
